s/Wrapper.js';\n\nfunction LoginPage() {\n  return (\n    <Wrapper>\n      <Login />\n    </Wrapper>\n  );\n}\n\nexport default LoginPage;\n","/Users/seandan/Desktop/blackjack-frontend/src/routes/GamePage.js",[],"/Users/seandan/Desktop/blackjack-frontend/src/routes/ProfilePage.js",[],"/Users/seandan/Desktop/blackjack-frontend/src/routes/RegisterPage.js",["113"],"import React from 'react';\n\nimport Header from '../components/Header.js';\nimport Register from '../components/Register.js';\nimport Wrapper from '../components/Wrapper.js';\n\nfunction RegisterPage() {\n  return (\n    <Wrapper>\n      <Register />\n    </Wrapper>\n  );\n}\n\nexport default RegisterPage;\n","/Users/seandan/Desktop/blackjack-frontend/src/routes/HomePage.js",["114","115","116"],"import React from 'react';\nimport {\n  GridItem,\n  Grid,\n  Center,\n  Button,\n  Box,\n} from '@chakra-ui/react';\nimport Header from '../components/Header';\nimport MyInfo from '../components/MyInfo';\nimport Leaderboard from '../components/Leaderboard/Leaderboard';\nimport Footer from '../components/Footer';\n\nconst HomePage = () => {\n  return (\n    <Grid\n      h=\"100vh\"\n      templateRows=\"repeat(12, 1fr)\"\n      templateColumns=\"repeat(12, 1fr)\"\n      bg=\"#8D89A6\"\n    >\n      <GridItem\n        rowStart={1}\n        rowEnd={2}\n        colStart={1}\n        colEnd={13}\n        boxShadow=\"dark-lg\"\n        rounded=\"md\"\n        bg=\"white\"\n      >\n        <Header />\n      </GridItem>\n      <GridItem\n        rowStart={2}\n        rowEnd={12}\n        colStart={1}\n        colEnd={3}\n        bg=\"#EACDC2\"\n      >\n        <MyInfo />\n      </GridItem>\n      <GridItem\n        rowStart={2}\n        rowEnd={12}\n        colStart={3}\n        colEnd={13}\n        boxShadow=\"dark-lg\"\n        rounded=\"md\"\n        bg=\"white\"\n        mt=\"20px\"\n        ml=\"50px\"\n        mr=\"50px\"\n      >\n        <Leaderboard />\n      </GridItem>\n      <GridItem\n        rowStart={12}\n